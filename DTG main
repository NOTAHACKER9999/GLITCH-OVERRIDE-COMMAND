e=document.querySelector('.subtitle'),title=document.querySelector('.title');let matrixEffectEnabled=false;canvas.width=window.innerWidth;canvas.height=window.innerHeight;const characters='ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789',fontSize=16,columns=canvas.width/fontSize;const drops=[];for(let x=0;x<columns;x++){drops[x]=1;}function draw(){if(!matrixEffectEnabled)return;ctx.fillStyle='rgba(0,0,0,0.1)';ctx.fillRect(0,0,canvas.width,canvas.height);ctx.fillStyle=currentColor;ctx.font=fontSize+'px monospace';for(let i=0;i<drops.length;i++){const text=characters.charAt(Math.floor(Math.random()*characters.length));ctx.fillText(text,i*fontSize,drops[i]*fontSize);if(drops[i]*fontSize>canvas.height&&Math.random()>0.975){drops[i]=0;}drops[i]++;}}let intervalId=null;function startMatrixEffect(){canvas.style.display='block';intervalId=setInterval(draw,33);}function stopMatrixEffect(){canvas.style.display='none';clearInterval(intervalId);intervalId=null;}subtitle.addEventListener('click',function(){matrixEffectEnabled=!matrixEffectEnabled;if(matrixEffectEnabled){startMatrixEffect();}else{stopMatrixEffect();}});const rainbowColors=['red','orange','yellow','green','blue','indigo','violet'];let colorIndex=0,currentColor='red';title.addEventListener('click',function(){if(matrixEffectEnabled){colorIndex=(colorIndex+1)%rainbowColors.length;currentColor=rainbowColors[colorIndex];}});const main_menu=document.getElementById('main-menu'),customization_menu=document.getElementById('customization-menu'),title_customization_menu=document.getElementById('title-customization-menu'),buttons_customization_menu=document.getElementById('buttons-customization-menu'),tab_hidders_menu=document.getElementById('tab-hidders-menu'),tabIcon=document.getElementById('tab-icon');function showMenu(menu){menu.style.display='flex';}function hideMenu(menu){menu.style.display='none';}const defaultTitleColor='#FF0000',defaultButtonColor='#FF0000';function applyDefaultStyles(){title.style.color=defaultTitleColor;const buttons=document.querySelectorAll('.button,.social-button');buttons.forEach(button=>{button.style.color=defaultButtonColor;button.style.borderColor=defaultButtonColor;});titleColorPicker.value=defaultTitleColor;buttonColorPicker.value=defaultButtonColor;}document.getElementById('customizers-button').addEventListener('click',function(){hideMenu(main_menu);showMenu(customization_menu);});document.getElementById('title-button').addEventListener('click',function(){hideMenu(customization_menu);showMenu(title_customization_menu);});document.getElementById('buttons-button').addEventListener('click',function(){hideMenu(customization_menu);showMenu(buttons_customization_menu);});document.getElementById('tab-hidders-button').addEventListener('click',function(){hideMenu(customization_menu);showMenu(tab_hidders_menu);});document.getElementById('back-to-main-button').addEventListener('click',function(){hideMenu(customization_menu);hideMenu(title_customization_menu);hideMenu(buttons_customization_menu);hideMenu(tab_hidders_menu);showMenu(main_menu);});document.getElementById('back-to-customization-title-button').addEventListener('click',function(){hideMenu(title_customization_menu);showMenu(customization_menu);});document.getElementById('back-to-customization-buttons-button').addEventListener('click',function(){hideMenu(buttons_customization_menu);showMenu(customization_menu);});document.getElementById('back-to-customization-tab-button').addEventListener('click',function(){hideMenu(tab_hidders_menu);showMenu(customization_menu);});document.getElementById('set-title-default-button').addEventListener('click',function(){titleColorPicker.value=defaultTitleColor;title.style.color=defaultTitleColor;});document.getElementById('set-buttons-default-button').addEventListener('click',function(){buttonColorPicker.value=defaultButtonColor;const buttons=document.querySelectorAll('.button,.social-button');buttons.forEach(button=>{button.style.color=defaultButtonColor;button.style.borderColor=defaultButtonColor;});});const titleColorPicker=document.getElementById('title-color-picker'),buttonColorPicker=document.getElementById('button-color-picker');titleColorPicker.addEventListener('input',function(event){title.style.color=event.target.value;saveSettings();});buttonColorPicker.addEventListener('input',function(event){const newColor=event.target.value;const buttons=document.querySelectorAll('.button,.social-button');buttons.forEach(button=>{button.style.color=newColor;button.style.borderColor=newColor;});saveSettings();});function saveSettings(){localStorage.setItem('titleColor',title.style.color);localStorage.setItem('buttonColor',buttonColorPicker.value);}function loadSettings(){const savedTitleColor=localStorage.getItem('titleColor'),savedButtonColor=localStorage.getItem('buttonColor');if(savedTitleColor){title.style.color=savedTitleColor;titleColorPicker.value=savedTitleColor;}if(savedButtonColor){const buttons=document.querySelectorAll('.button,.social-button');buttons.forEach(button=>{button.style.color=savedButtonColor;button.style.borderColor=savedButtonColor;});buttonColorPicker.value=savedButtonColor;}}applyDefaultStyles();loadSettings();tab_hidders_menu.addEventListener('click', function(event) {if (event.target.classList.contains('menu-button')) {const tabTitle = event.target.dataset.tabTitle;const tabIconUrl = event.target.dataset.tabIcon;document.title = tabTitle;tabIcon.href = tabIconUrl;console.log('Title set to:', tabTitle);console.log('Icon set to:', tabIconUrl);}});</script></body></html>
